<p id="notice"><%= notice %></p>

<h1>Type Cameras</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_cameras.each do |type_camera| %>
      <tr>
        <td><%= type_camera.name %></td>
        <td><%= link_to 'Show', type_camera %></td>
        <td><%= link_to 'Edit', edit_type_camera_path(type_camera) %></td>
        <td><%= link_to 'Destroy', type_camera, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Camera', new_type_camera_path %>
